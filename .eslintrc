{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true,
    },
    "rules": {
        semi: ["error", "never"],
        "indent": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "jsx-a11y/no-static-element-interactions": 0,
    }
}
